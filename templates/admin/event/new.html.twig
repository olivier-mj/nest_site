{% extends 'admin.html.twig' %}

{% block title %}New Event{% endblock %}

{% block stylesheets %}
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/tom-select@2.0.3/dist/css/tom-select.css">
<link href="https://cdn.jsdelivr.net/npm/suneditor@latest/dist/css/suneditor.min.css" rel="stylesheet">
{% endblock %}

{% block body %}
    <h1>Create new Event</h1>
    {{ form_start(form) }}
    {% set delete = false %}
    {{ include('admin/event/_form.html.twig',{'new': 'block'}) }}
    {{ form_end(form) }}
    

    <a href="{{ path('app_event_index') }}">back to list</a>
{% endblock %}
{% block javascripts %}
{{parent() }}

<script src="https://cdn.jsdelivr.net/npm/suneditor@latest/dist/suneditor.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/suneditor@latest/src/lang/fr.js"></script>
<script src="https://cdn.jsdelivr.net/npm/suneditor@latest/src/lang/en.js"></script>
{% endblock %}